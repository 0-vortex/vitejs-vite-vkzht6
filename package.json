{
  "name": "vitejs-open-sauced-stackblitz",
  "description": "Open Sauced provides structured onboarding for new contributors to open source. This structure provides a way to track your next contributions by leveraging a unique dashboard built on top of the GitHub GraphQL API.",
  "version": "0.41.1",
  "private": true,
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "bugs": "https://github.com/0-vortex/vitejs-open-sauced-stackblitz/issues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/0-vortex/vitejs-open-sauced-stackblitz.git"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "push": "npx @open-sauced/conventional-commit",
    "start": "npx vite",
    "build": "npx vite build",
    "serve": "npx vite preview",
    "build:optimize": "npx svgo src/**/*.svg --config ./.svgorc.js",
    "build:analyze": "npm run build -- --mode report",
    "test": "npx jest --config ./.jest/jest.config.js",
    "coverage": "npm test -- --coverage --watchAll=false",
    "storybook": "npx start-storybook -p 6006",
    "lint": "npx eslint . --report-unused-disable-directives",
    "format": "npm run lint -- --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@primer/octicons-react": "^16.2.0",
    "@remirror/extension-bold": "^1.0.12",
    "@remirror/extension-code": "^1.0.13",
    "@remirror/extension-emoji": "^1.0.13",
    "@remirror/extension-heading": "^1.0.12",
    "@remirror/extension-italic": "^1.0.12",
    "@remirror/extension-list": "^1.2.9",
    "@remirror/extension-markdown": "^1.0.12",
    "@remirror/extension-react-tables": "^1.0.19",
    "@remirror/pm": "^1.0.9",
    "@remirror/react": "^1.0.19",
    "@remirror/react-components": "^1.0.18",
    "@remirror/styles": "^1.1.2",
    "contrast": "^1.0.1",
    "dayjs": "^1.10.7",
    "graphql": "^16.2.0",
    "humanize-duration": "^3.27.1",
    "onegraph-apollo-client": "^3.1.2",
    "onegraph-auth": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-keys": "^2.7.1",
    "react-loading-skeleton": "^3.0.1",
    "react-markdown": "^7.1.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "styled-components": "^5.3.3",
    "svgmoji": "^3.2.0",
    "swr": "^1.1.1"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^1.3.5",
    "@open-sauced/check-engines": "^1.2.0",
    "@open-sauced/conventional-commit": "^1.0.1",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-storysource": "^6.4.9",
    "@storybook/api": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.1.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.4.5",
    "babel-preset-react-app": "^10.0.1",
    "camelcase": "^6.2.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "jest": "^27.4.5",
    "jest-axe": "^5.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "postcss": "^8.4.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.1.0",
    "react-app-polyfill": "^3.0.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "storybook-dark-mode": "^1.0.8",
    "vite": "^2.7.6",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-pwa": "^0.11.13",
    "web-vitals": "^2.1.2"
  }
}
